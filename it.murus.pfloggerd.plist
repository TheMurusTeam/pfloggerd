<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>it.murus.pfloggerd</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/sh</string>
		<string>-c</string>
		<string>/usr/sbin/ipconfig waitall; /sbin/ifconfig pflog0 create; /usr/local/bin/pfloggerd</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>ExitTimeOut</key>
	<integer>1</integer>
	<key>KeepAlive</key>
	<false/>
</dict>
</plist>
